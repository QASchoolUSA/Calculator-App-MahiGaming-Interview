{
    "compilerOptions": {
      "target": "ES6",
      "module": "esnext",
      "lib": ["dom", "ES2016"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "moduleResolution": "node"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }